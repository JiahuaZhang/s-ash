{
  "name": "i-slash",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "unocss './app/**/*.{ts,tsx}' --out-file ./app/styles/uno.css",
    "dev": "concurrently \"remix dev --manual\" \"npm run dev:css\"",
    "dev:css": "npm run build:css -- --watch",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@unocss/reset": "^0.56.5",
    "isbot": "^3.6.8",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-social-media-embed": "^2.3.6",
    "slate": "^0.94.1",
    "slate-react": "^0.99.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/prismjs": "^1.26.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "typescript": "^5.1.6",
    "unocss": "^0.56.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
